<% if @videos != [] %>
  <h2>Videos</h2>
  <ul>
    <% @videos.each do |video| %>
      <li><%= link_to video.search_term_name, video.watch_link, target:"_blank" %></li>
      <% if video.notes != [] %>
        <% video.notes.each do |note| %>
          <%= note.body %>
        <% end %>
      <% end %>
    <% end %>
  </ul>
<% end %>

<% if @bookmarks != [] %>
  <h2>Bookmarks</h2>
  <ul>
    <% @bookmarks.each do |bookmark| %>
      <li><%= link_to bookmark.url, bookmark.url, target:"_blank" %></li>
      <% if bookmark.notes != [] %>
        <% bookmark.notes.each do |note| %>
          <%= note.body %>
        <% end %>
      <% end %>
    <% end %>
  </ul>
<% end %>

<% if @misc_notes != [] %>
  <h2>Notes</h2>
  <ul>
    <% @misc_notes.each do |note| %>
      <li><%= note.body %></li>
    <% end %>
  </ul>
<% end %>
